!2017 github

program DikUcgenAlanCevre
implicit none
real*8::a,b,c,alan,cevre

write(*,*) "Birinci dik kenar a:"
read(*,*) a

write(*,*) "Ikinci dik kenar b:"
read(*,*) b

  c=sqrt(a**2+b**2)
  alan=(a*b)/2.0
  cevre=a+b+c

 write(*,*) a,b,c
 write(*,*) "alan:",alan," cevre:", cevre

end program DikUcgenAlanCevre
